{
  "name": "super-simple-react-toast",
  "version": "1.1.0",
  "description": "A simple react toast app",
  "repository": "jaehyeon48/super-simple-react-toast",
  "homepage": "https://github.com/jaehyeon48/super-simple-react-toast#readme",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "private": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project ./tsconfig.publish.json",
    "build:cjs": "tsc --project ./tsconfig.publish.json --module commonjs --outDir dist/cjs",
    "clean": "rimraf dist",
    "lint": "eslint ./src/lib",
    "test": "jest --env=jsdom"
  },
  "keywords": [
    "react",
    "toast",
    "toast-message",
    "notification",
    "alert"
  ],
  "author": "jaehyeon48 <iamjaehyeon48@gmail.com> (https://github.com/jaehyeon48)",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.2",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/styled-components": "^5.1.25",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "babel-jest": "^28.1.0",
    "eslint": "^8.16.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.2.2",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.0",
    "nanoid": "^3.3.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "styled-components": "^5.3.5",
    "ts-jest": "^28.0.3",
    "typescript": "^4.6.4"
  }
}
